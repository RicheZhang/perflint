{
  "name": "perflint",
  "version": "0.0.1",
  "author": "Matthew Harrison-Jones <matt+npm@matthojo.co.uk>",
  "description": "A Web page performance checker using WebPageTest.",
  "bin": {
    "perflint": "./bin/perflint.js"
  },
  "scripts": {
    "test": "istanbul cover ./node_modules/mocha/bin/_mocha --report lcovonly -- -R spec ./tests/*.js",
    "pretest": "npm run-script lint",
    "posttest": "(istanbul check-coverage --statements 90 --branches 90 --functions 100 --lines 90 && rm -rf coverage)",
    "coveralls": "cat ./coverage/lcov.info | coveralls",
    "lint": "eslint ."
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/perflint/perflint.git"
  },
  "keywords": [
    "lint",
    "performance",
    "website",
    "webpagetest"
  ],
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/perflint/perflint/issues"
  },
  "homepage": "https://github.com/perflint/perflint#readme",
  "devDependencies": {
    "chai": "^3.5.0",
    "coveralls": "^2.11.6",
    "eslint": "^2.2.0",
    "istanbul": "^0.4.2",
    "mocha": "^2.4.5"
  },
  "dependencies": {
    "chalk": "^1.1.1",
    "cli": "^0.11.1",
    "exit": "^0.1.2",
    "shelljs": "^0.6.0"
  }
}
