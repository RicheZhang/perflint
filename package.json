{
  "name": "perflint",
  "version": "0.0.6",
  "author": "Matthew Harrison-Jones <matt+npm@matthojo.co.uk>",
  "description": "A Web page performance checker using WebPageTest.",
  "bin": {
    "perflint": "./bin/perflint.js"
  },
  "scripts": {
    "test": "nyc ./node_modules/mocha/bin/_mocha --  ./tests/lib/**/*.js && nyc report --reporter=text-lcov > lcov.info",
    "pretest": "npm run-script lint",
    "check-coverage": "(nyc check-coverage --statements 90 --branches 90 --functions 100 --lines 90)",
    "coveralls": "cat ./lcov.info | coveralls",
    "lint": "eslint ."
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/perflint/perflint.git"
  },
  "keywords": [
    "lint",
    "performance",
    "website",
    "webpagetest"
  ],
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/perflint/perflint/issues"
  },
  "homepage": "https://github.com/perflint/perflint#readme",
  "devDependencies": {
    "chai": "^3.5.0",
    "coveralls": "^2.11.6",
    "eslint": "^2.2.0",
    "mocha": "^2.4.5",
    "nyc": "^6.6.1",
    "proxyquire": "^1.7.4",
    "sinon": "^1.17.3",
    "strip-ansi": "^3.0.1"
  },
  "dependencies": {
    "async": "^2.0.0-rc.2",
    "chalk": "^1.1.1",
    "cli": "^0.11.2",
    "debug": "^2.2.0",
    "js-yaml": "^3.6.1",
    "lodash": "^4.5.1",
    "lodash.escape": "^4.0.0",
    "progress": "^1.1.8",
    "shelljs": "^0.7.0",
    "text-table": "^0.2.0",
    "webpagetest": "^0.3.4"
  },
  "engines": {
    "node": ">=4.4.6"
  }
}
